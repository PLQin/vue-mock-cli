{
  "name": "vue-mock-cli",
  "version": "1.0.0",
  "description": "vue-cli vs mock data",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PLQin/vue-mock-cli.git"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "mock": "nodemon --watch mock --exec json-server mock/index.js --port 3020 --m mock/post-to-get.js",
    "win:mockdev": "npm run mock | npm run dev",
    "mac:mockdev": "npm run mock & npm run dev"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "json-server": "^0.16.1",
    "lodash": "^4.17.19",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^2.0.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.19.1",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^1.1.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^11.0.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "Vue",
    "Mock",
    "template"
  ],
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 12.18.0",
    "npm": ">= 6.14.0"
  },
  "browserslist": [
    "> 5%",
    "not ie < 11",
    "last 2 versions"
  ]
}